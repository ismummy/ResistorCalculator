        </activity>
        <activity android:name="edu.lautech.student.resistor.ValueCalculator"></activity>
        <activity android:name="edu.lautech.student.resistor.ColorCalculator"></activity>
        <activity android:name="edu.lautech.student.resistor.About"></activity>
